<Window x:Class="SQ_TeamClack_TermProj.admin_LogFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQ_TeamClack_TermProj"
        mc:Ignorable="d"
        Title="Log File" Height="450" Width="800" WindowStyle="ToolWindow">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF343434" Offset="0" />
            <GradientStop Color="#FF3E3E3E" Offset="1" />
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20*" />
            <ColumnDefinition Width="13*" />
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="LogFileDisplayBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="400" Width="772" Foreground="White" Grid.ColumnSpan="2" />
    </Grid>
</Window>